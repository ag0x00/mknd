@prefix : <https://schema.mekanoid.io/mekanoid-d3fend.ttl#> .
@prefix d3f: <http://d3fend.mitre.org/ontologies/d3fend.owl#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix mknd: <https://schema.mekanoid.io/mekanoid.ttl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@base <https://schema.mekanoid.io/mekanoid-d3fend.ttl#> .

<https://schema.mekanoid.io/mekanoid-d3fend.ttl#> rdf:type owl:Ontology ;
                                                   owl:imports d3f: ,
                                                               <https://schema.mekanoid.io/mekanoid.ttl#> ;
                                                   dct:created "2022-09-19"^^xsd:date ;
                                                   dct:creator <mailto:anton.goncharov@mekanoid.io> ;
                                                   dct:license "MIT" ;
                                                   dct:modified "2023-09-19"^^xsd:date ;
                                                   dct:title "Mekanoid's expanded version of MITRE D3FEND ontology" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/created
dct:created rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
dct:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/modified
dct:modified rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://d3fend.mitre.org/ontologies/d3fend.owl#analyzed-by
d3f:analyzed-by rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf d3f:d3fend-catalog-object-property ;
                owl:inverseOf d3f:analyzes ;
                rdfs:label "analyzed by"@en .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#analyzes
d3f:analyzes owl:inverseOf d3f:isolated-by .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#associated-with
d3f:associated-with rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#connected-by
d3f:connected-by rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf d3f:d3fend-catalog-object-property ;
                 owl:inverseOf d3f:connects ;
                 rdfs:label "connected by"@en .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#enables
d3f:enables owl:inverseOf d3f:has-goal .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#has-goal
d3f:has-goal rdfs:range d3f:UseCaseGoal .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#identified-by
d3f:identified-by owl:inverseOf d3f:identifies .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#isolated-by
d3f:isolated-by rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf d3f:d3fend-catalog-object-property ;
                rdfs:label "isolated by"@en .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#may-be-associated-with
d3f:may-be-associated-with rdf:type owl:ObjectProperty ,
                                    owl:SymmetricProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#providedBy
d3f:providedBy rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf d3f:d3fend-catalog-object-property ;
               owl:inverseOf d3f:provides ;
               rdfs:label "provided by"@en .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#recorded-in
d3f:recorded-in owl:inverseOf d3f:records .


#################################################################
#    Data properties
#################################################################

###  http://d3fend.mitre.org/ontologies/d3fend.owl#identifier
d3f:identifier owl:equivalentProperty <https://schema.mekanoid.io/mekanoid.owl#external-id> ;
               rdfs:subPropertyOf <https://schema.mekanoid.io/mekanoid.owl#vendorProperty> .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#version
d3f:version rdfs:subPropertyOf <https://schema.mekanoid.io/mekanoid.owl#vulnProperty> .


###  https://schema.mekanoid.io/mekanoid.owl#external-id
<https://schema.mekanoid.io/mekanoid.owl#external-id> rdf:type owl:DatatypeProperty .


###  https://schema.mekanoid.io/mekanoid.owl#vendorProperty
<https://schema.mekanoid.io/mekanoid.owl#vendorProperty> rdf:type owl:DatatypeProperty .


###  https://schema.mekanoid.io/mekanoid.owl#vulnProperty
<https://schema.mekanoid.io/mekanoid.owl#vulnProperty> rdf:type owl:DatatypeProperty .


#################################################################
#    Classes
#################################################################

###  http://d3fend.mitre.org/ontologies/d3fend.owl#Command
d3f:Command rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty d3f:has-goal ;
                              owl:someValuesFrom d3f:UseCaseGoal
                            ] .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#ExecutableBinary
d3f:ExecutableBinary rdfs:subClassOf [ rdf:type owl:Restriction ;
                                       owl:onProperty d3f:enables ;
                                       owl:someValuesFrom d3f:Subroutine
                                     ] .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#Log
d3f:Log rdfs:subClassOf d3f:Record .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#Software
d3f:Software rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty d3f:may-contain ;
                               owl:someValuesFrom d3f:Vulnerability
                             ] .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#URL
d3f:URL rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty d3f:identifies ;
                          owl:someValuesFrom <https://schema.mekanoid.io/mekanoid.owl#Malware>
                        ] .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#UserAccount
d3f:UserAccount rdfs:subClassOf [ rdf:type owl:Restriction ;
                                  owl:onProperty d3f:uses ;
                                  owl:someValuesFrom <https://schema.mekanoid.io/mekanoid.owl#Username>
                                ] .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#UserAction
d3f:UserAction owl:disjointWith <https://schema.mekanoid.io/mekanoid.owl#MachineAction> .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#Vulnerability
d3f:Vulnerability rdfs:subClassOf [ rdf:type owl:Restriction ;
                                    owl:onProperty d3f:may-be-contained-by ;
                                    owl:someValuesFrom d3f:Software
                                  ] .


###  https://schema.mekanoid.io/mekanoid.owl#MachineAction
<https://schema.mekanoid.io/mekanoid.owl#MachineAction> rdf:type owl:Class .


###  https://schema.mekanoid.io/mekanoid.owl#Malware
<https://schema.mekanoid.io/mekanoid.owl#Malware> rdf:type owl:Class .


###  https://schema.mekanoid.io/mekanoid.owl#Username
<https://schema.mekanoid.io/mekanoid.owl#Username> rdf:type owl:Class .


#################################################################
#    Individuals
#################################################################

###  https://schema.mekanoid.io/mekanoid.owl#Upload
<https://schema.mekanoid.io/mekanoid.owl#Upload> rdf:type owl:NamedIndividual ,
                                                          d3f:UseCaseGoal .


#################################################################
#    Annotations
#################################################################

d3f:Email skos:altLabel "Email Message"@en .


d3f:Procedure rdfs:label "Procedure"@en .


