<?xml version="1.0"?>
<rdf:RDF xmlns="https://mekanoid.io/ontology/"
     xml:base="https://mekanoid.io/ontology/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="https://mekanoid.io/ontology/">
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://github.com/usnistgov/vulntology/</rdfs:isDefinedBy>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://ontology.mekanoid.io/vulntology/affectsProduct -->


    <owl:ObjectProperty rdf:about="https://ontology.mekanoid.io/vulntology/affectsProduct"/>
    


    <!-- https://ontology.mekanoid.io/vulntology/blockedByBarrier -->


    <owl:ObjectProperty rdf:about="https://ontology.mekanoid.io/vulntology/blockedByBarrier"/>
    


    <!-- https://ontology.mekanoid.io/vulntology/doesNotResultInImpact -->


    <owl:ObjectProperty rdf:about="https://ontology.mekanoid.io/vulntology/doesNotResultInImpact">
        <owl:propertyDisjointWith rdf:resource="https://ontology.mekanoid.io/vulntology/resultsInImpact"/>
        <rdfs:comment>Impacts will not occur due to an Action.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasAction -->


    <owl:ObjectProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasAction"/>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasEntityRole -->


    <owl:ObjectProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasEntityRole">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://ontology.mekanoid.io/vulntology/ActionObject"/>
        <rdfs:range rdf:resource="https://ontology.mekanoid.io/vulntology/ImpactObject"/>
    </owl:ObjectProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasIdentity -->


    <owl:ObjectProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasIdentity"/>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasKnownChain -->


    <owl:ObjectProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasKnownChain">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasOriginatingProduct -->


    <owl:ObjectProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasOriginatingProduct"/>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasScenario -->


    <owl:ObjectProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasScenario"/>
    


    <!-- https://ontology.mekanoid.io/vulntology/resultsInImpact -->


    <owl:ObjectProperty rdf:about="https://ontology.mekanoid.io/vulntology/resultsInImpact">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Impacts will occur due to an Action.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://ontology.mekanoid.io/vulntology/affectsContext -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/affectsContext">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:comment>The conceptual entity where the impacts are realized from successful exploitation of a security vulnerability. Different impacts can be realized by multiple contexts from multiple scenarios.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/evidencedBySource -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/evidencedBySource">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasBarrierType -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasBarrierType">
        <rdfs:comment>Identifies the kind of barrier. Based on the barrier&apos;s type, additional properties may be required.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasCriticality -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasCriticality">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:comment>A measure of the relative importance of the associated Scope.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasCve5Product -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasCve5Product">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:comment>This is to reference the CVE Program CVE 5 JSON&apos;s product section, which can communicate many complicated methods of string based product applicability.</rdfs:comment>
        <rdfs:label xml:lang="en">CVE v5 Product</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasEngineeringMethod -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasEngineeringMethod">
        <rdfs:comment>The method or mechanism used to manipulate a user into interacting with a malicious resource.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasEntityRole -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasEntityRole">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasEscapeContext -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasEscapeContext">
        <rdfs:comment>The association denotes where a sandbox breakout originated.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasExploitedWeakness -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasExploitedWeakness">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasGainedPrivilege -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasGainedPrivilege">
        <rdfs:comment>Abstraction to assist in capturing relative privilege levels. The abstraction is only for the sake of discussing the vulnerability and is not intended to communicate the actual granular privileges that exist in most information system environments.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasImpactMethod -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasImpactMethod">
        <rdfs:comment>A description of the method used to exploit a vulnerability providing some additional information on the impact of exploitation.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasImpactMethodType -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasImpactMethodType">
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment>The nature of impact.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasLocation -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasLocation">
        <rdfs:comment>Designating the specific area or location impacted. Serves as supplemental information</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasLogicalImpact -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasLogicalImpact">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:comment>Impacts that occur to the digital aspects of the software. These are considered for assessing traditional notions of confidentiality, integrity and availability.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasName -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasName">
        <rdfs:comment>A name or label to assist in identifying a given action in the context of the containing Vulnerability. This name should be unique across all sibling actions.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasNeededPrivilege -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasNeededPrivilege">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:comment>The privileges that are needed relative to the type of barrier being overcome.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasNvdCpeApplicabilityStatement -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasNvdCpeApplicabilityStatement">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:comment>This is to reference the NVD configurations section, which requires much more complex JSON than simple strings.</rdfs:comment>
        <rdfs:label>NVD CPE Applicability Statement</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasPhysicalImpact -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasPhysicalImpact">
        <rdfs:comment>A tangible impact to a physical device, machinery, the surrounding environment, or people.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasProductEnumeration -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasProductEnumeration">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:comment>The enumeration of one or many products as dictated by the identification scheme. Contains a scheme and value pair. This is intended to be used for simple enumerations such as generic free text or common formats that identify explicit instances of products such as CPE or SWID.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasScope -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasScope">
        <rdfs:comment>A coarse measure of the level of impact an exploit could have on a target</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/hasSectorOfInterest -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/hasSectorOfInterest">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/id -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/id">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>A globally unique identifier for the action that distinguishes it from other actions related to the same vulnerability.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/relatesToContext -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/relatesToContext">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:comment>The context to which the privileges are related</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/requiresAttackTheater -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/requiresAttackTheater">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:comment>The Theatre or Attack Theater is the area or place from which an attack must occur. Each separate theater represents varying levels of implied trust and attack surface.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/scheme -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/scheme">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:label>Scheme</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://ontology.mekanoid.io/vulntology/value -->


    <owl:DatatypeProperty rdf:about="https://ontology.mekanoid.io/vulntology/value">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:label>Value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://ontology.mekanoid.io/vulntology/ActionObject -->


    <owl:Class rdf:about="https://ontology.mekanoid.io/vulntology/ActionObject">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/doesNotResultInImpact"/>
                <owl:someValuesFrom rdf:resource="https://ontology.mekanoid.io/vulntology/ImpactObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/resultsInImpact"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="https://ontology.mekanoid.io/vulntology/ImpactObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/affectsContext"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/hasEntityRole"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/id"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/hasImpactMethod"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/hasName"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An Action is what occurs within a given Scenario. Actions involve a high level impact method which occur in a specific context. Actions can result in impacts to the context referenced. If multiple contexts are affected there will be multiple actions each relating to a given context.</rdfs:comment>
        <rdfs:label>Action</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.mekanoid.io/vulntology/BarrierObject -->


    <owl:Class rdf:about="https://ontology.mekanoid.io/vulntology/BarrierObject">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/hasEngineeringMethod"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/hasNeededPrivilege"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/relatesToContext"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/hasBarrierType"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/id"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>This could be any characteristic inherent in the vulnerability that could impede the adversary from achieving successful exploitation or a protection mechanism that may limit the actions or impact that can be taken even if the vulnerability is able to be exploited. These are often part of the system in which the product is deployed or are inherent in how the product is used.</rdfs:comment>
        <rdfs:label>Barrier</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.mekanoid.io/vulntology/ImpactMethodObject -->


    <owl:Class rdf:about="https://ontology.mekanoid.io/vulntology/ImpactMethodObject">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/hasEscapeContext"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/hasGainedPrivilege"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/hasImpactMethodType"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A description of the method used to exploit a vulnerability providing some additional information on the impact of exploitation. These are intended to be high level concepts that lead to more granular impacts as referenced in the logical and physical impact entities.</rdfs:comment>
        <rdfs:label>Impact Method</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.mekanoid.io/vulntology/ImpactObject -->


    <owl:Class rdf:about="https://ontology.mekanoid.io/vulntology/ImpactObject">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/hasCriticality"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/hasLogicalImpact"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/hasScope"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/id"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/hasLocation"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An Impact is a recognized result of an Action for a given Vulnerability Scenario.</rdfs:comment>
        <rdfs:label>Impact</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.mekanoid.io/vulntology/ProductObject -->


    <owl:Class rdf:about="https://ontology.mekanoid.io/vulntology/ProductObject">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/hasCve5Product"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/hasNvdCpeApplicabilityStatement"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/hasProductEnumeration"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>The software and/or hardware configurations that are known to be vulnerable to exploitation of the Vulnerability Scenario. Different Product configurations can be associated with different Scenarios to allow for description of varying impacts and exploitation mechanisms.</rdfs:comment>
        <rdfs:label>Product</rdfs:label>
    </owl:Class>
    


    <!-- https://ontology.mekanoid.io/vulntology/ScenarioObject -->


    <owl:Class rdf:about="https://ontology.mekanoid.io/vulntology/ScenarioObject">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/blockedByBarrier"/>
                <owl:someValuesFrom rdf:resource="https://ontology.mekanoid.io/vulntology/BarrierObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ontology.mekanoid.io/vulntology/affectsProduct"/>
                <owl:allValuesFrom rdf:resource="https://ontology.mekanoid.io/vulntology/ProductObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
