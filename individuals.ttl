###  http://d3fend.mitre.org/ontologies/d3fend.owl#T1003.001
d3f:T1003.001 rdf:type d3f:T1003.001 ,
                       :SubTechnique ;
              d3f:uses :Direct_Access_and_Read .


###  https://schema.mekanoid.io/Command
:Command rdf:type owl:NamedIndividual ,
                  :Function ;
         rdfs:comment "It can be used to break out from restricted environments by running non-interactive system commands."@en ;
         rdfs:label "Command"@en .


###  https://schema.mekanoid.io/Direct_Access_and_Read
:Direct_Access_and_Read rdf:type owl:NamedIndividual ,
                                 :Procedure ;
                        d3f:uses :Process_Access ,
                                 :Process_Enumeration ,
                                 :Process_Read ;
                        rdfs:label "Direct Access and Read"@en .


###  https://schema.mekanoid.io/ExecutableBinary_000009
:ExecutableBinary_000009 rdf:type owl:NamedIndividual ,
                                  d3f:ExecutableBinary ;
                         d3f:enables :file-read ,
                                     :file-write ,
                                     :limited-suid ,
                                     :non-interactive-bind-shell ,
                                     :non-interactive-reverse-shell ,
                                     :shell ,
                                     :sudo ,
                                     :suid ;
                         :indicatedBy :awk ;
                         rdfs:comment "Unix binary"@en ;
                         rdfs:label "awk"@en .


###  https://schema.mekanoid.io/Handle_Copy
:Handle_Copy rdf:type owl:NamedIndividual ,
                      :Operation ;
             rdfs:label "Handle Copy"@en .


