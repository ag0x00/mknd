d3f:analyzed-by rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf d3f:d3fend-catalog-object-property ;
                owl:inverseOf d3f:analyzes ;
                rdfs:label "analyzed by"@en .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#analyzes
d3f:analyzes rdf:type owl:ObjectProperty ;
             owl:inverseOf d3f:isolated-by .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#associated-with
d3f:associated-with rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#connected-by
d3f:connected-by rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf d3f:d3fend-catalog-object-property ;
                 owl:inverseOf d3f:connects ;
                 rdfs:label "connected by"@en .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#connects
d3f:connects rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#contained-by
d3f:contained-by rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#contains
d3f:contains rdf:type owl:ObjectProperty .


