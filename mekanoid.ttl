@prefix : <https://mekanoid.io/ontology/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix d3f: <http://d3fend.mitre.org/ontologies/d3fend.owl#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix time: <http://www.w3.org/2006/time#> .
@base <https://mekanoid.io/ontology/> .

<https://mekanoid.io/ontology/> rdf:type owl:Ontology ;
                                 owl:versionIRI <https://mekanoid.io/ontology/0.3.0/> ;
                                 owl:imports <http://d3fend.mitre.org/ontologies/d3fend.owl> ;
                                 dct:created "2022-04-02"^^xsd:date ;
                                 dct:description "Mekanoid Knowledge Network Dictionary (MKND) is a public ontology for semantic knowledge graph applications. Its purpose is to make possible trusted and autonomous cybersecurity threat prevention and response. It is a superset of the D3FEND ontology, and is designed to be used in conjunction with it. Mekanoid is a work in progress, and is subject to change."@en ;
                                 dct:modified "2024-01-16"^^xsd:date ;
                                 dct:title "MKND" ;
                                 rdfs:comment "Use of the MKND, and the associated references from this ontology are subject to the Terms of Use. Copyright 2021 Neon Lane Co DBA Mekanoid Corporation." ;
                                 owl:versionInfo "0.3.0" .

#################################################################
#    Annotation properties
#################################################################

###  http://d3fend.mitre.org/ontologies/d3fend.owl#definition
d3f:definition rdf:type owl:AnnotationProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#name
d3f:name rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/created
dct:created rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
dct:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
dct:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dct:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/modified
dct:modified rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
dct:title rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#qualifiedCardinality
owl:qualifiedCardinality rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#altLabel
skos:altLabel rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#example
skos:example rdf:type owl:AnnotationProperty .


###  https://mekanoid.io/ontology/context
:context rdf:type owl:AnnotationProperty ;
         rdfs:subPropertyOf rdfs:comment .


###  https://mekanoid.io/ontology/icon
:icon rdf:type owl:AnnotationProperty ;
      rdfs:label "has icon"@en ;
      rdfs:range xsd:anyURI ;
      rdfs:domain owl:Thing .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://d3fend.mitre.org/ontologies/d3fend.owl#accessed-by
d3f:accessed-by rdf:type owl:ObjectProperty ;
                owl:inverseOf d3f:accesses .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#accesses
d3f:accesses rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#analyzed-by
d3f:analyzed-by rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf d3f:d3fend-catalog-object-property ;
                owl:inverseOf d3f:analyzes ;
                rdfs:label "analyzed by"@en .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#analyzes
d3f:analyzes rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#associated-with
d3f:associated-with rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf d3f:may-be-associated-with .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#connected-by
d3f:connected-by rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf d3f:d3fend-catalog-object-property ;
                 owl:inverseOf d3f:connects ;
                 rdfs:label "connected by"@en .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#connects
d3f:connects rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#contained-by
d3f:contained-by rdf:type owl:ObjectProperty ;
                 owl:inverseOf d3f:contains .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#contains
d3f:contains rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#created-by
d3f:created-by rdf:type owl:ObjectProperty ;
               owl:inverseOf d3f:creates .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#creates
d3f:creates rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#d3fend-catalog-object-property
d3f:d3fend-catalog-object-property rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#enabled-by
d3f:enabled-by rdf:type owl:ObjectProperty ;
               owl:inverseOf d3f:enables .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#enables
d3f:enables rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#evaluates
d3f:evaluates rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#executes
d3f:executes rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#has-goal
d3f:has-goal rdf:type owl:ObjectProperty ;
             rdfs:range d3f:UseCaseGoal .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#identified-by
d3f:identified-by rdf:type owl:ObjectProperty ;
                  owl:inverseOf d3f:identifies .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#identifies
d3f:identifies rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#invoked-by
d3f:invoked-by rdf:type owl:ObjectProperty ;
               owl:inverseOf d3f:invokes .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#invokes
d3f:invokes rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#isolated-by
d3f:isolated-by rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf d3f:d3fend-catalog-object-property ;
                rdfs:label "isolated by"@en .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#may-be-associated-with
d3f:may-be-associated-with rdf:type owl:ObjectProperty ,
                                    owl:SymmetricProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#may-be-contained-by
d3f:may-be-contained-by rdf:type owl:ObjectProperty ;
                        owl:inverseOf d3f:may-contain .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#may-contain
d3f:may-contain rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#modified-by
d3f:modified-by rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#produces
d3f:produces rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#providedBy
d3f:providedBy rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf d3f:d3fend-catalog-object-property ;
               owl:inverseOf d3f:provides ;
               rdfs:label "provided by"@en .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#provides
d3f:provides rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#recorded-in
d3f:recorded-in rdf:type owl:ObjectProperty ;
                owl:inverseOf d3f:records .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#records
d3f:records rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#updates
d3f:updates rdf:type owl:ObjectProperty ;
            owl:inverseOf :updated-by .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#used-by
d3f:used-by rdf:type owl:ObjectProperty ;
            owl:inverseOf d3f:uses .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#uses
d3f:uses rdf:type owl:ObjectProperty .


###  https://mekanoid.io/ontology/attackComplexity
:attackComplexity rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :hasCVSSMetric ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:range :CVSS-AttackComplexity ;
                  rdfs:label "attack complexity" .


###  https://mekanoid.io/ontology/attackVector
:attackVector rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasCVSSMetric ;
              rdf:type owl:FunctionalProperty ;
              rdfs:range :CVSS-AttackVector .


###  https://mekanoid.io/ontology/availabilityImpact
:availabilityImpact rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :hasCVSSMetric ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:range :CVSS-AvailabilityImpact .


###  https://mekanoid.io/ontology/confidentialityImpact
:confidentialityImpact rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :hasCVSSMetric ;
                       rdf:type owl:FunctionalProperty .


###  https://mekanoid.io/ontology/exploitCodeMaturity
:exploitCodeMaturity rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :hasCVSSMetric ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:range :CVSS-ExploitCodeMaturity ;
                     rdfs:label "exploit code maturity" .


###  https://mekanoid.io/ontology/hasCVSSMetric
:hasCVSSMetric rdf:type owl:ObjectProperty ;
               rdfs:domain d3f:Vulnerability ;
               rdfs:label "has CVSS metric" .


###  https://mekanoid.io/ontology/hasURL
:hasURL rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :reference ;
        rdfs:domain d3f:File ,
                    :Report .


###  https://mekanoid.io/ontology/importBucketUrl
:importBucketUrl rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :hasURL .


###  https://mekanoid.io/ontology/indicatedBy
:indicatedBy rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :mayBeIndicatedBy ;
             owl:inverseOf :indicates ;
             rdfs:label "indicated by"@en .


###  https://mekanoid.io/ontology/indicates
:indicates rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :mayIndicate ;
           rdfs:label "indicates"@en .


###  https://mekanoid.io/ontology/integrityImpact
:integrityImpact rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :hasCVSSMetric ;
                 rdf:type owl:FunctionalProperty .


###  https://mekanoid.io/ontology/mayBeIndicatedBy
:mayBeIndicatedBy rdf:type owl:ObjectProperty ;
                  owl:inverseOf :mayIndicate ;
                  rdfs:label "may be indicated by"@en .


###  https://mekanoid.io/ontology/mayIndicate
:mayIndicate rdf:type owl:ObjectProperty ;
             rdfs:label "may indicate"@en .


###  https://mekanoid.io/ontology/privilegesRequired
:privilegesRequired rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :hasCVSSMetric ;
                    rdf:type owl:FunctionalProperty .


###  https://mekanoid.io/ontology/reference
:reference rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty .


###  https://mekanoid.io/ontology/remediationLevel
:remediationLevel rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :hasCVSSMetric ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:range :CVSS-RemediationLevel ;
                  rdfs:label "remediation level" .


###  https://mekanoid.io/ontology/reportConfidence
:reportConfidence rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :hasCVSSMetric ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:range :CVSS-ReportConfidence ;
                  rdfs:label "report confidence" .


###  https://mekanoid.io/ontology/scope
:scope rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :hasCVSSMetric ;
       rdf:type owl:FunctionalProperty .


###  https://mekanoid.io/ontology/severityRating
:severityRating rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :hasCVSSMetric ;
                rdf:type owl:FunctionalProperty ;
                rdfs:range :CVSS-SeverityType ;
                rdfs:label "severity" .


###  https://mekanoid.io/ontology/updated-by
:updated-by rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf d3f:modified-by ;
            rdfs:label "updated by" .


###  https://mekanoid.io/ontology/userInteraction
:userInteraction rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :hasCVSSMetric ;
                 rdf:type owl:FunctionalProperty .


#################################################################
#    Data properties
#################################################################

###  http://d3fend.mitre.org/ontologies/d3fend.owl#identifier
d3f:identifier rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty .


###  https://mekanoid.io/ontology/cpe
:cpe rdf:type owl:DatatypeProperty ;
     rdfs:subPropertyOf d3f:identifier ;
     rdfs:domain d3f:Software ;
     rdfs:range xsd:string ;
     rdfs:label "CPE" ;
     skos:example "cpe:2.3:a:apple:icloud:1.0:*:*:*:*:macos:*:*" .


###  https://mekanoid.io/ontology/cvssBaseScore
:cvssBaseScore rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain d3f:Vulnerability ;
               rdfs:range xsd:decimal ;
               rdfs:label "base score" .


###  https://mekanoid.io/ontology/cvssExploitabilityScore
:cvssExploitabilityScore rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf owl:topDataProperty ;
                         rdf:type owl:FunctionalProperty ;
                         rdfs:domain d3f:Vulnerability ;
                         rdfs:range xsd:decimal ;
                         rdfs:label "exploitability score" .


###  https://mekanoid.io/ontology/cvssImpactScore
:cvssImpactScore rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdf:type owl:FunctionalProperty ;
                 rdfs:domain d3f:Vulnerability ;
                 rdfs:range xsd:decimal ;
                 rdfs:label "impact score" .


###  https://mekanoid.io/ontology/dateTime
:dateTime rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :MekaThing ;
          rdfs:range xsd:dateTimeStamp ;
          rdfs:label "has timestamp"@en ;
          skos:definition "A date and time associated with the entity"@en ;
          skos:example "2002-10-10T12:00:00-05:00" .


###  https://mekanoid.io/ontology/dateTimeEpoch
:dateTimeEpoch rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:range xsd:long ;
               rdfs:comment "A Unix epoch timestamp associated with the entity"@en ;
               rdfs:label "has epoch"@en ;
               skos:definition "A Unix epoch timestamp associated with the entity"@en ;
               skos:example "990577200"^^xsd:long .


###  https://mekanoid.io/ontology/extCategory
:extCategory rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :vendorProperty ;
             rdfs:label "category" .


###  https://mekanoid.io/ontology/extClassification
:extClassification rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :vendorProperty ;
                   rdfs:comment "An external classification that may apply to the artifact. Typically provided by a 3rd party vendor."@en ;
                   rdfs:label "classification"@en .


###  https://mekanoid.io/ontology/extPriority
:extPriority rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :extClassification ;
             rdfs:comment "Priority as defined by the 3rd party vendor responsible for creating the Artifact."@en ;
             rdfs:label "Priority"@en .


###  https://mekanoid.io/ontology/extRisk
:extRisk rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :extClassification ;
         rdfs:comment "Risk as defined by the 3rd party vendor responsible for creating the Artifact."@en ;
         rdfs:label "Risk"@en .


###  https://mekanoid.io/ontology/extSeverity
:extSeverity rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :extClassification ;
             rdfs:comment "Severity as defined by the 3rd party vendor responsible for creating the Artifact."@en ;
             rdfs:label "Severity"@en .


###  https://mekanoid.io/ontology/extStatus
:extStatus rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :vendorProperty ;
           rdfs:comment "Status of the Artifact as defined by the 3rd party vendor software responsible for generating it."@en ;
           rdfs:label "status"@en .


###  https://mekanoid.io/ontology/extThreat
:extThreat rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :extClassification ;
           rdfs:comment "Threat Level as defined by the 3rd party vendor responcible for creating the Artifact."@en ;
           rdfs:label "Threat Level"@en .


###  https://mekanoid.io/ontology/extType
:extType rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :vendorProperty ;
         rdfs:label "type" .


###  https://mekanoid.io/ontology/extUrgency
:extUrgency rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf :extClassification ;
            rdfs:comment "Urgency as defined by the 3rd party vendor responsible for generating the Artifact."@en ;
            rdfs:label "Urgency"@en .


###  https://mekanoid.io/ontology/external-id
:external-id rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf d3f:identifier ,
                                :vendorProperty ;
             rdfs:domain d3f:Host ,
                         d3f:UserAccount ;
             rdfs:label "external ID"@en ;
             skos:altLabel "vendor ID" .


###  https://mekanoid.io/ontology/firstSeen
:firstSeen rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :timeSeen ;
           rdfs:comment "The first time the enity had been observed by the application."@en ;
           rdfs:label "first seen"@en .


###  https://mekanoid.io/ontology/firstSeenEpoch
:firstSeenEpoch rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf :dateTimeEpoch ;
                rdfs:label "first seen"@en .


###  https://mekanoid.io/ontology/hasURL
:hasURL rdf:type owl:DatatypeProperty .


###  https://mekanoid.io/ontology/id
:id rdf:type owl:DatatypeProperty ;
    rdfs:subPropertyOf d3f:identifier ;
    rdfs:label "ID"@en .


###  https://mekanoid.io/ontology/importBucketName
:importBucketName rdf:type owl:DatatypeProperty ;
                  rdfs:label "S3 bucket"@en .


###  https://mekanoid.io/ontology/importBucketUrl
:importBucketUrl rdf:type owl:DatatypeProperty .


###  https://mekanoid.io/ontology/importFileName
:importFileName rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf owl:topDataProperty ;
                rdfs:label "filename"@en .


###  https://mekanoid.io/ontology/importId
:importId rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf d3f:identifier ;
          rdfs:label "import ID"@en .


###  https://mekanoid.io/ontology/lastAccessedEpoch
:lastAccessedEpoch rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :dateTimeEpoch ;
                   rdfs:label "last accessed"@en .


###  https://mekanoid.io/ontology/lastSeen
:lastSeen rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :timeSeen ;
          rdfs:comment "The last time the entity had been observed by the application."@en ;
          rdfs:label "last seen"@en .


###  https://mekanoid.io/ontology/lastSeenEpoch
:lastSeenEpoch rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :dateTimeEpoch ;
               rdfs:label "last seen"@en .


###  https://mekanoid.io/ontology/lastStatusChangeEpoch
:lastStatusChangeEpoch rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf :dateTimeEpoch ;
                       rdfs:label "last status change"@en .


###  https://mekanoid.io/ontology/lastUpdated
:lastUpdated rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :updated ;
             rdfs:label "last updated at" ;
             skos:altLabel "last modified at" .


###  https://mekanoid.io/ontology/lastUpdatedEpoch
:lastUpdatedEpoch rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf :dateTimeEpoch ;
                  rdfs:label "last modified"@en .


###  https://mekanoid.io/ontology/last_modified
:last_modified rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :updated ;
               rdfs:label "last modified" .


###  https://mekanoid.io/ontology/reference
:reference rdf:type owl:DatatypeProperty .


###  https://mekanoid.io/ontology/size
:size rdf:type owl:DatatypeProperty ,
               owl:FunctionalProperty ;
      rdfs:domain d3f:File ;
      rdfs:range xsd:byte ;
      rdfs:label "has size"@en .


###  https://mekanoid.io/ontology/stringValue
:stringValue rdf:type owl:DatatypeProperty ;
             rdfs:domain :Observable ;
             rdfs:range xsd:string ;
             rdfs:comment "the object is represented by the following string value"@en ;
             rdfs:label "is"@en .


###  https://mekanoid.io/ontology/teamId
:teamId rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf d3f:identifier ;
        rdfs:label "team ID"@en .


###  https://mekanoid.io/ontology/templateId
:templateId rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf d3f:identifier ;
            rdfs:label "template ID"@en .


###  https://mekanoid.io/ontology/timeCreated
:timeCreated rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :dateTime ;
             rdfs:comment "Time when the entity had been created at the point of origin."@en ;
             rdfs:label "created at"@en .


###  https://mekanoid.io/ontology/timeCreatedEpoch
:timeCreatedEpoch rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf :dateTimeEpoch ;
                  rdfs:comment "Time when the entity had been created at the point of origin."@en ;
                  rdfs:label "created at"@en .


###  https://mekanoid.io/ontology/timeSeen
:timeSeen rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :dateTime ;
          rdfs:comment "A timestamp associated with a point in time when the entity had been observed by the application in some way."@en ;
          rdfs:label "seen at"@en .


###  https://mekanoid.io/ontology/updated
:updated rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf :dateTime ;
         rdfs:label "modified at" ,
                    "updated at" ;
         skos:altLabel "modified at" .


###  https://mekanoid.io/ontology/userId
:userId rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf d3f:identifier ;
        rdfs:label "user ID"@en .


###  https://mekanoid.io/ontology/usesNetworkPort
:usesNetworkPort rdf:type owl:DatatypeProperty ;
                 rdfs:range xsd:positiveInteger ;
                 rdfs:comment "uses network port number" ;
                 rdfs:label "uses port" .


###  https://mekanoid.io/ontology/value
