                                                      owl:onProperty d3fend:assesses ;
                                                      owl:someValuesFrom d3fend:DefensiveTechniqueClaim
                                                    ] ,
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty d3fend:counters ;
