@prefix : <http://d3fend.mitre.org/ontologies/d3fend.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix d3fend: <http://d3fend.mitre.org/ontologies/d3fend.owl#> .
@base <http://d3fend.mitre.org/ontologies/d3fend.owl#> .

<http://d3fend.mitre.org/ontologies/d3fend.owl> rdf:type owl:Ontology ;
                                                 owl:versionIRI <http://d3fend.mitre.org/ontologies/d3fend/0.13.0-BETA-1/d3fend.owl> ;
                                                 d3fend:release-date "2023-10-30T00:00:00+00:00"^^xsd:dateTime ;
                                                 terms:description "D3FEND is a framework which encodes a countermeasure knowledge base as a knowledge graph. The graph contains the types and relations that define key concepts in the cybersecurity countermeasure domain and the relations necessary to link those concepts to each other. Each of these concepts and relations are linked to references in the cybersecurity literature." ;
                                                 terms:title "D3FEND™ - A knowledge graph of cybersecurity countermeasures" ;
                                                 rdfs:comment "Use of the D3FEND Knowledge Graph, and the associated references from this ontology are subject to the Terms of Use. D3FEND is funded by the National Security Agency (NSA) Cybersecurity Directorate and managed by the National Security Engineering Center (NSEC) which is operated by The MITRE Corporation. D3FEND™ and the D3FEND logo are trademarks of The MITRE Corporation. This software was produced for the U.S. Government under Basic Contract No. W56KGU-18-D0004, and is subject to the Rights in Noncommercial Computer Software and Noncommercial Computer Software Documentation Clause 252.227-7014 (FEB 2012) Copyright 2022 The MITRE Corporation." ;
                                                 owl:versionInfo "0.13.0-BETA-1" .

#################################################################
#    Annotation properties
#################################################################

###  http://d3fend.mitre.org/ontologies/d3fend.owl#attack-id
d3fend:attack-id rdf:type owl:AnnotationProperty ;
                 d3fend:definition "x attack-id y: The offensive technique x has the att&ck unique id of y." ;
                 rdfs:label "attack-id" ;
                 rdfs:subPropertyOf d3fend:attack-kb-annotation ;
                 rdfs:range xsd:string ;
                 rdfs:domain d3fend:OffensiveTechnique .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#attack-kb-annotation
d3fend:attack-kb-annotation rdf:type owl:AnnotationProperty ;
                            d3fend:definition "x attack-kb-annotation y: The offensive technique x has the kb annotation of y." ;
                            rdfs:label "attack-kb-annotation" ;
                            rdfs:subPropertyOf d3fend:d3fend-annotation ;
                            rdfs:domain d3fend:OffensiveTechnique .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#cwe-id
d3fend:cwe-id rdf:type owl:AnnotationProperty ;
              rdfs:label "cwe-id" ;
              rdfs:subPropertyOf d3fend:cwe-kb-annotation .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#cwe-kb-annotation
d3fend:cwe-kb-annotation rdf:type owl:AnnotationProperty ;
                         rdfs:label "cwe-kb-annotation" ;
                         rdfs:subPropertyOf d3fend:d3fend-annotation .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#d3fend-annotation
d3fend:d3fend-annotation rdf:type owl:AnnotationProperty ;
                         d3fend:definition "x d3fend-annotation y: The d3fend object x has the annotation y." ;
                         rdfs:label "d3fend-annotation" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#d3fend-catalog-annotation-property
d3fend:d3fend-catalog-annotation-property rdf:type owl:AnnotationProperty ;
                                          rdfs:label "d3fend-catalog-annotation-property" ;
                                          skos:altLabel "d3fend-vendor-registry-annotation-property" ;
                                          rdfs:subPropertyOf d3fend:d3fend-annotation .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#d3fend-display-annotation
d3fend:d3fend-display-annotation rdf:type owl:AnnotationProperty ;
                                 rdfs:label "d3fend-display-annotation" ;
                                 rdfs:subPropertyOf d3fend:d3fend-annotation .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#d3fend-kb-annotation-property
d3fend:d3fend-kb-annotation-property rdf:type owl:AnnotationProperty ;
                                     d3fend:definition "x d3fend-kb-annotation-property y: The entity x had the d3fend kb annotation y." ;
                                     rdfs:label "d3fend-kb-annotation-property" ;
                                     rdfs:subPropertyOf d3fend:d3fend-annotation .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#d3fend-kb-reference-annotation
d3fend:d3fend-kb-reference-annotation rdf:type owl:AnnotationProperty ;
                                      d3fend:definition "x d3fend-kb-data-property y: The reference x has the data property y." ;
                                      rdfs:label "d3fend-kb-reference-annotation" ;
                                      rdfs:subPropertyOf d3fend:d3fend-kb-annotation-property ;
                                      rdfs:range xsd:string ;
                                      rdfs:domain d3fend:Reference .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#definition
d3fend:definition rdf:type owl:AnnotationProperty ;
                  d3fend:definition "x definition y: The d3fend object x has the definition y." ;
                  rdfs:label "comment" ,
                             "definition" ;
                  rdfs:subPropertyOf d3fend:d3fend-annotation .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#description
d3fend:description rdf:type owl:AnnotationProperty ;
                   d3fend:definition "A statement that represents something in words." ;
                   rdfs:label "description"@en ;
                   rdfs:subPropertyOf d3fend:d3fend-catalog-annotation-property .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#display-baseurl
d3fend:display-baseurl rdf:type owl:AnnotationProperty ;
                       d3fend:definition "A base string to use as prefix to create a full URL for an entity. The baseurl must end in a forward slash: /" ;
                       rdfs:label "display-baseurl" ;
                       rdfs:subPropertyOf d3fend:d3fend-display-annotation .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#display-order
d3fend:display-order rdf:type owl:AnnotationProperty ;
                     d3fend:definition "x display-order y: An object x should be displayed in ordinal position y when placed or listed in a d3fend display with other objects of its kind." ;
                     rdfs:label "display-order" ;
                     rdfs:subPropertyOf d3fend:d3fend-display-annotation .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#display-priority
d3fend:display-priority rdf:type owl:AnnotationProperty ;
                        rdfs:label "display-priority" ;
                        rdfs:subPropertyOf d3fend:d3fend-display-annotation .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#kb-abstract
d3fend:kb-abstract rdf:type owl:AnnotationProperty ;
                   d3fend:definition "x kb-abstract y: The reference x has the abstract y." ;
                   rdfs:label "kb-abstract" ;
                   rdfs:subPropertyOf d3fend:d3fend-kb-reference-annotation ;
                   rdfs:range xsd:string ;
                   rdfs:domain d3fend:Reference .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#kb-article
d3fend:kb-article rdf:type owl:AnnotationProperty ;
                  d3fend:definition "The technique x has the kb-article y, where y is written in Markdown." ;
                  rdfs:label "kb-article" ;
                  rdfs:subPropertyOf d3fend:d3fend-kb-reference-annotation ;
                  rdfs:range xsd:string ;
                  rdfs:domain d3fend:Technique .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#kb-author
d3fend:kb-author rdf:type owl:AnnotationProperty ;
                 d3fend:definition "x kb-author y: The reference x has some author y." ;
                 rdfs:label "kb-author" ;
                 rdfs:subPropertyOf d3fend:d3fend-kb-reference-annotation ;
                 rdfs:range xsd:string ;
                 rdfs:domain d3fend:Reference .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#kb-mitre-analysis
d3fend:kb-mitre-analysis rdf:type owl:AnnotationProperty ;
                         d3fend:definition "x kb-mitre-analysis y: The reference x has the mitre d3fend analysis y." ;
                         rdfs:label "kb-mitre-analysis" ;
                         rdfs:subPropertyOf d3fend:d3fend-kb-annotation-property ;
                         rdfs:range xsd:string ;
                         rdfs:domain d3fend:Reference .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#kb-organization
d3fend:kb-organization rdf:type owl:AnnotationProperty ;
                       d3fend:definition "x kb-organization y: The reference x was created or owned by the organization y." ;
                       rdfs:label "kb-organization" ;
                       rdfs:subPropertyOf d3fend:d3fend-kb-annotation-property .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#label
d3fend:label rdf:type owl:AnnotationProperty ;
             rdfs:label "label"@en ;
             rdfs:subPropertyOf rdfs:label .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#pref-label
d3fend:pref-label rdf:type owl:AnnotationProperty ;
                  d3fend:definition "x pref-label y: The preferred display value for x is y in d3fend tools." ;
                  rdfs:label "pref-label" ;
                  rdfs:subPropertyOf d3fend:d3fend-annotation .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#release-date
d3fend:release-date rdf:type owl:AnnotationProperty ;
                    d3fend:definition "x release-date y: The object x has the release-date y." ;
                    rdfs:label "release-date" ;
                    rdfs:subPropertyOf d3fend:d3fend-annotation ,
                                       owl:versionInfo .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#synonym
d3fend:synonym rdf:type owl:AnnotationProperty ;
               d3fend:definition "an equivalent term." ;
               rdfs:label "synonym" ;
               rdfs:subPropertyOf d3fend:d3fend-annotation .


###  http://purl.org/dc/terms/description
terms:description rdf:type owl:AnnotationProperty ;
                  rdfs:label "description" .


###  http://purl.org/dc/terms/title
terms:title rdf:type owl:AnnotationProperty ;
            rdfs:label "title" .


###  http://www.w3.org/2000/01/rdf-schema#label
rdfs:label rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#seeAlso
rdfs:seeAlso rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#versionInfo
owl:versionInfo rdfs:subPropertyOf d3fend:d3fend-annotation .


###  http://www.w3.org/2004/02/skos/core#altLabel
skos:altLabel rdf:type owl:AnnotationProperty ;
              rdfs:label "altLabel"@en .


#################################################################
#    Object Properties
#################################################################

###  http://d3fend.mitre.org/ontologies/d3fend.owl#abuses
d3fend:abuses rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf d3fend:uses ;
              d3fend:definition "x abuses y: The entity x applies an artifact y to a wrong thing or person; x applies y badly or incorrectly." ;
              rdfs:label "abuses" ;
              skos:altLabel "misapplies" ,
                            "misuses" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#accessed-by
d3fend:accessed-by rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf d3fend:associated-with ,
                                      d3fend:may-be-accessed-by ;
                   owl:inverseOf d3fend:accesses ;
                   rdfs:label "accessed-by" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#accesses
d3fend:accesses rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf d3fend:associated-with ,
                                   d3fend:may-access ;
                rdfs:range d3fend:NetworkResource ;
                d3fend:definition "x accesses y: An subject x takes the action of reading from, writing into, or executing the stored information in the object y. Reads, writes, and executes are specific cases of accesses." ;
                rdfs:label "accesses" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#addressed-by
d3fend:addressed-by rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf d3fend:associated-with ;
                    owl:inverseOf d3fend:addresses ;
                    rdfs:domain d3fend:Resource ;
                    rdfs:range d3fend:Identifier ;
                    d3fend:definition "x addressed-by y: Relates a resource x (e.g., network host, peripheral device, disk sector, a memory cell or other logical or physical entity) to a discrete address y in an address space that points to it." ;
                    rdfs:label "addressed-by" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#addresses
d3fend:addresses rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf d3fend:associated-with ;
                 rdfs:domain d3fend:Identifier ;
                 rdfs:range d3fend:Resource ;
                 d3fend:definition "x addresses y: Relates a pointer x to a digital artifact y located in the address space to which x points. The address space is part of some digital store, whether it be in memory, an image, or a persistent storage device." ;
                 rdfs:label "addresses" ;
                 rdfs:seeAlso <http://dbpedia.org/resource/Pointer_(computer_programming)> ,
                              <http://wordnet-rdf.princeton.edu/id/02253826-v> ;
                 skos:altLabel "points-to" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#adds
d3fend:adds rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf d3fend:associated-with ,
                               d3fend:may-add ;
            d3fend:definition "x adds y: The subject x adds a data object y, such as a file, to some other digital artifact, such as a directory. Examples include an agent or technique adding a record to a database. or a domain entry to a DNS server." ;
            rdfs:label "adds" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#analyzes
d3fend:analyzes rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf d3fend:associated-with ,
                                   d3fend:detects ;
                d3fend:definition "x analyzes y: The subject x break down object y into components or essential features, assessing y by quantitative methods, qualitative methods, or both.  Usually the analysis is done in terms of some model or framework." ;
                rdfs:label "analyzes" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#assessed-by
d3fend:assessed-by rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf d3fend:d3fend-catalog-object-property ;
                   owl:inverseOf d3fend:assesses ;
                   rdfs:label "assessed-by" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#assesses
d3fend:assesses rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf d3fend:d3fend-catalog-object-property ;
                rdfs:domain d3fend:DefensiveTechniqueClaim ;
                rdfs:range d3fend:DefensiveTechniqueAssessment ;
                rdfs:label "assesses" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#associated-with
d3fend:associated-with rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf d3fend:may-be-associated-with ;
                       d3fend:definition "x associated-with y: The subject x and object y are associated in some way.  This is the most general definite relationship in d3fend (i.e., most general relationship that is not prefixed by 'may-'.)" ;
                       rdfs:label "associated-with" ;
                       rdfs:seeAlso <http://wordnet-rdf.princeton.edu/id/13804981-n> .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#attached-to
d3fend:attached-to rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf d3fend:associated-with ;
                   d3fend:definition "x attached-to y: A subject x is joined in close association to an object y." ;
                   rdfs:label "attached-to" ;
                   rdfs:seeAlso "d3f:connects" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#attack-may-be-countered-by
d3fend:attack-may-be-countered-by rdf:type owl:ObjectProperty ;
                                  rdfs:subPropertyOf d3fend:may-be-tactically-associated-with ;
                                  rdfs:label "attack-may-be-countered-by" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#authenticates
d3fend:authenticates rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf d3fend:associated-with ,
                                        d3fend:hardens ;
                     d3fend:definition "x authenticates y: The subject x establishes the authenticity of some y. This relation indicates an authentication event has occurred." ;
                     rdfs:label "authenticates" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#author
d3fend:author rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf d3fend:creator ;
              rdfs:label "author" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#authorizes
d3fend:authorizes rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf d3fend:associated-with ,
                                     d3fend:hardens ;
                  d3fend:definition "x authorizes y: A subject x grants authorization or clearance for an agent y to use an object.  This relation indicates an authorization event has occurred." ;
                  rdfs:label "authorizes" ;
                  rdfs:seeAlso <http://wordnet-rdf.princeton.edu/id/00805664-v> .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#blocks
d3fend:blocks rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf d3fend:counters ,
                                 d3fend:filters ;
              d3fend:definition "x blocks y: The entity x blocks off the use of digital artifact y by reference to a block or allow list (or both.)" ;
              rdfs:label "blocks" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#broader
d3fend:broader rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf d3fend:semantic-relation ;
               rdfs:label "broader" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#broader-transitive
d3fend:broader-transitive rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf d3fend:semantic-relation ;
                          rdfs:label "broader-transitive" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#cited-by
d3fend:cited-by rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf d3fend:d3fend-catalog-object-property ;
                owl:inverseOf d3fend:cites ;
                rdfs:label "cited-by" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#cites
d3fend:cites rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf d3fend:d3fend-catalog-object-property ;
             owl:inverseOf d3fend:d3fend-catalog-object-property ;
             rdfs:range d3fend:InformationContentEntity ;
             rdfs:label "cites" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#claimed-by
d3fend:claimed-by rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf d3fend:d3fend-catalog-object-property ;
                  owl:inverseOf d3fend:claims ;
                  rdfs:label "claimed-by" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#claims
d3fend:claims rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf d3fend:d3fend-catalog-object-property ;
              rdfs:label "claims" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#configures
d3fend:configures rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf d3fend:associated-with ,
                                     d3fend:hardens ;
                  rdfs:label "configures" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#connects
d3fend:connects rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf d3fend:associated-with ;
                d3fend:definition "x connects y: The subject x joins system y by means of communication equipment (to some other system, typically the adversary-targeted host)." ;
                rdfs:label "connects" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#contained-by
d3fend:contained-by rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf d3fend:associated-with ,
                                       d3fend:may-be-contained-by ;
                    owl:inverseOf d3fend:contains ;
                    rdf:type owl:TransitiveProperty ;
                    rdfs:label "contained-by" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#contains
d3fend:contains rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf d3fend:associated-with ,
                                   d3fend:may-contain ;
                rdf:type owl:TransitiveProperty ;
                d3fend:definition "x contains y: A core relation that holds between a whole x and its part y.  Equivalent to relational concept 'has part' and thus transitive." ;
                rdfs:label "contains" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#contributor
d3fend:contributor rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf d3fend:d3fend-catalog-object-property ;
                   rdfs:range owl:Thing .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#copies
d3fend:copies rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf d3fend:creates ;
              d3fend:definition "x copies y: An technique or agent x reproduces or makes and exact copy of some digital artifact y." ;
              rdfs:label "copies" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#copy-of
d3fend:copy-of rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf d3fend:associated-with ;
               d3fend:definition "x copy-of y: The subject x is a duplicate of the object y" ;
               rdfs:label "copy-of" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#counters
d3fend:counters rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf d3fend:d3fend-catalog-object-property ,
                                   d3fend:may-counter ;
                rdfs:label "counters" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#created-by
d3fend:created-by rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf d3fend:associated-with ,
                                     d3fend:may-be-created-by ;
                  owl:inverseOf d3fend:creates ;
                  rdfs:label "created-by" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#creates
d3fend:creates rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf d3fend:associated-with ,
                                  d3fend:may-create ;
               d3fend:definition "x creates y: The subject x bring into existence an object y.  Some technique or agent x creates a persistent digital artifact y (as opposed to production of a consumable or transient object.); i.e., bring forth or generate" ;
               rdfs:label "creates" ;
               rdfs:seeAlso "produces" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#creator
d3fend:creator rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf d3fend:contributor ;
               rdfs:label "creator" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#d3fend-catalog-object-property
d3fend:d3fend-catalog-object-property rdf:type owl:ObjectProperty ;
                                      rdfs:subPropertyOf d3fend:d3fend-object-property ;
                                      rdfs:label "d3fend-catalog-object-property" ;
                                      skos:altLabel "d3fend-vendor-registry-object-property" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#d3fend-general-object-property
d3fend:d3fend-general-object-property rdf:type owl:ObjectProperty ;
                                      rdfs:subPropertyOf d3fend:d3fend-object-property ;
                                      rdfs:label "d3fend-general-object-property" .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#d3fend-kb-object-property
d3fend:d3fend-kb-object-property rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf d3fend:d3fend-object-property ;
                                 d3fend:definition "x d3fend-kb-object-property y: The object y is a d3fend knowledge base object property. These properties allow the linkage of knowledge and information supporting and illustrating the d3fend model." ;
                                 rdfs:label "d3fend-kb-object-property" .


