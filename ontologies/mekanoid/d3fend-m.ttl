                                                    d3fend:has-link "https://car.mitre.org/analytics/CAR-2019-07-001/"^^xsd:anyURI ;
                                                    d3fend:kb-reference-title "CAR-2019-07-001: Access Permission Modification" ;
                                                    d3fend:kb-abstract "Adversaries sometimes modify object access rights at the operating system level. There are varying motivations behind this action - they may not want some files/objects to be changed on systems for persistence reasons and therefore provide admin only rights; also, they may want files to be accessible with lower levels of permissions." ;
                                                    d3fend:kb-author "MITRE" ;
                                                    d3fend:kb-mitre-analysis "" ;
                                                    d3fend:kb-organization "MITRE" ;
                                                    rdfs:label "Reference - CAR-2019-07-001: Access Permission Modification - MITRE" .


