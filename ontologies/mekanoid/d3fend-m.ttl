                                                      owl:someValuesFrom xsd:integer
                                                    ] ,
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty d3fend:rating ;
                                                      owl:allValuesFrom [ rdf:type rdfs:Datatype ;
                                                                          owl:oneOf [ rdf:type rdf:List ;
                                                                                      rdf:first "0" ;
                                                                                      rdf:rest [ rdf:type rdf:List ;
                                                                                                 rdf:first "1" ;
                                                                                                 rdf:rest [ rdf:type rdf:List ;
                                                                                                            rdf:first "2" ;
                                                                                                            rdf:rest [ rdf:type rdf:List ;
                                                                                                                       rdf:first "3" ;
                                                                                                                       rdf:rest rdf:nil
                                                                                                                     ]
                                                                                                          ]
                                                                                               ]
                                                                                    ]
                                                                        ]
                                                    ] ,
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty d3fend:stage ;
                                                      owl:allValuesFrom [ rdf:type rdfs:Datatype ;
                                                                          owl:oneOf [ rdf:type rdf:List ;
                                                                                      rdf:first "Deceive" ;
                                                                                      rdf:rest [ rdf:type rdf:List ;
                                                                                                 rdf:first "Detect" ;
                                                                                                 rdf:rest [ rdf:type rdf:List ;
                                                                                                            rdf:first "Evict" ;
                                                                                                            rdf:rest [ rdf:type rdf:List ;
                                                                                                                       rdf:first "Harden" ;
                                                                                                                       rdf:rest [ rdf:type rdf:List ;
                                                                                                                                  rdf:first "Isolate" ;
                                                                                                                                  rdf:rest rdf:nil
                                                                                                                                ]
                                                                                                                     ]
                                                                                                          ]
                                                                                               ]
                                                                                    ]
                                                                        ]
                                                    ] ,
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty d3fend:rating ;
                                                      owl:cardinality "1"^^xsd:nonNegativeInteger
                                                    ] ,
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty d3fend:stage ;
                                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onDataRange xsd:string
                                                    ] ;
                                    d3fend:definition "Assessing how well a capability implementation's capability feature functions as a countermeasure." ;
