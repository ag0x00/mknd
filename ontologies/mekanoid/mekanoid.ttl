d3f:isolated-by rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf d3f:d3fend-catalog-object-property ;
                rdfs:label "isolated by"@en .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#may-be-associated-with
d3f:may-be-associated-with rdf:type owl:ObjectProperty ,
                                    owl:SymmetricProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#may-be-contained-by
d3f:may-be-contained-by rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#may-contain
d3f:may-contain rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#modified-by
d3f:modified-by rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#produces
d3f:produces rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#providedBy
d3f:providedBy rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf d3f:d3fend-catalog-object-property ;
               owl:inverseOf d3f:provides ;
               rdfs:label "provided by"@en .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#provides
d3f:provides rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#recorded-in
d3f:recorded-in rdf:type owl:ObjectProperty ;
                owl:inverseOf d3f:records .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#records
d3f:records rdf:type owl:ObjectProperty .


###  http://d3fend.mitre.org/ontologies/d3fend.owl#updates
d3f:updates rdf:type owl:ObjectProperty ;
