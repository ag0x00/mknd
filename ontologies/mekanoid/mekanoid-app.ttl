#################################################################
#    Classes
#################################################################

###  https://schema.mekanoid.io/mekanoid.ttl#AutomatedTaskRun
mknd:AutomatedTaskRun rdf:type owl:Class ;
                      rdfs:subClassOf mknd:MachineAction ,
                                      mknd:TaskRun ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty d3f:invoked-by ;
                                        owl:allValuesFrom mknd:TaskPattern
                                      ] ;
                      rdfs:label "Automated Task Run" .


###  https://schema.mekanoid.io/mekanoid.ttl#Connection
mknd:Connection rdf:type owl:Class ;
                rdfs:subClassOf mknd:SystemObject ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty d3f:enables ;
                                  owl:someValuesFrom mknd:Task
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty d3f:enables ;
                                  owl:allValuesFrom mknd:Integration
                                ] ;
                mknd:icon "connection.svg"^^xsd:anyURI .


###  https://schema.mekanoid.io/mekanoid.ttl#DataTemplate
mknd:DataTemplate rdf:type owl:Class ;
                  rdfs:subClassOf mknd:SystemObject ;
                  rdfs:label "Data Template" ;
                  mknd:icon "file.svg"^^xsd:anyURI .


###  https://schema.mekanoid.io/mekanoid.ttl#Integration
mknd:Integration rdf:type owl:Class ;
                 rdfs:subClassOf mknd:SystemObject ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty d3f:contains ;
                                   owl:someValuesFrom mknd:Task
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty d3f:enabled-by ;
                                   owl:someValuesFrom mknd:Connection
                                 ] .


###  https://schema.mekanoid.io/mekanoid.ttl#ManualTaskRun
mknd:ManualTaskRun rdf:type owl:Class ;
                   rdfs:subClassOf d3f:UserAction ,
                                   mknd:TaskRun ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty d3f:invoked-by ;
                                     owl:allValuesFrom mknd:AppUser
                                   ] ;
                   rdfs:label "Manual Task Run" .


###  https://schema.mekanoid.io/mekanoid.ttl#MekaThing
mknd:MekaThing mknd:icon "mekanoid.svg"^^xsd:anyURI .


###  https://schema.mekanoid.io/mekanoid.ttl#Report
mknd:Report rdfs:subClassOf d3f:InformationContentEntity ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty d3f:produces ;
                              owl:allValuesFrom mknd:Verdict
                            ] ;
            mknd:icon "doc.svg"^^xsd:anyURI .


###  https://schema.mekanoid.io/mekanoid.ttl#SystemObject
mknd:SystemObject rdf:type owl:Class ;
                  rdfs:subClassOf mknd:MekaThing ;
                  rdfs:comment "Mekanoid System Object" ;
                  rdfs:label "Meka Object" ;
                  mknd:icon "mekanoid.svg"^^xsd:anyURI .


###  https://schema.mekanoid.io/mekanoid.ttl#Task
mknd:Task rdf:type owl:Class ;
          rdfs:subClassOf mknd:SystemObject ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty d3f:contained-by ;
                            owl:someValuesFrom mknd:Integration
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty d3f:uses ;
                            owl:someValuesFrom mknd:DataTemplate
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty d3f:uses ;
                            owl:allValuesFrom mknd:TaskTemplate
                          ] ;
          rdfs:comment "An abstract concept. Something to be done." ;
          skos:altLabel "Atomic Task" ;
          mknd:icon "task.svg"^^xsd:anyURI .


###  https://schema.mekanoid.io/mekanoid.ttl#TaskPattern
mknd:TaskPattern rdf:type owl:Class ;
                 rdfs:subClassOf mknd:SystemObject ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty d3f:created-by ;
                                   owl:someValuesFrom mknd:AppUser
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty d3f:invokes ;
                                   owl:someValuesFrom mknd:AutomatedTaskRun
                                 ] ;
                 rdfs:label "Task Pattern" ;
                 mknd:icon "flow.svg"^^xsd:anyURI .


###  https://schema.mekanoid.io/mekanoid.ttl#TaskRun
mknd:TaskRun rdf:type owl:Class ;
             rdfs:subClassOf d3f:DigitalEvent ,
                             mknd:SystemObject ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty d3f:creates ;
                               owl:someValuesFrom mknd:TaskRunLog
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty d3f:uses ;
                               owl:someValuesFrom mknd:DataTemplate
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty d3f:uses ;
                               owl:someValuesFrom mknd:TaskTemplate
                             ] ;
             rdfs:label "Task Run" ;
             mknd:icon "task.svg"^^xsd:anyURI .


###  https://schema.mekanoid.io/mekanoid.ttl#TaskRunLog
mknd:TaskRunLog rdf:type owl:Class ;
                rdfs:subClassOf d3f:EventLog ,
                                mknd:SystemObject ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty d3f:created-by ;
                                  owl:someValuesFrom mknd:TaskRun
                                ] ;
                rdfs:label "Task Run Log" ;
                mknd:icon "log.svg"^^xsd:anyURI .


###  https://schema.mekanoid.io/mekanoid.ttl#TaskTemplate
mknd:TaskTemplate rdf:type owl:Class ;
                  rdfs:subClassOf mknd:SystemObject ;
                  rdfs:label "Task Template" ;
                  mknd:icon "file.svg"^^xsd:anyURI .


###  https://schema.mekanoid.io/mekanoid.ttl#Tenant
mknd:Tenant rdf:type owl:Class ;
            rdfs:subClassOf mknd:SystemObject ;
            rdfs:comment "Mekanoid Tenant" ;
            mknd:icon "biz-unit.svg"^^xsd:anyURI .


