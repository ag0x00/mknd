# version 2024-03-29

@prefix : <https://mekanoid.io/ontology/cvss/> .
@prefix d3f:  <http://d3fend.mitre.org/ontologies/d3fend.owl#> .
@prefix mknd: <https://mekanoid.io/ontology/> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .

###  https://mekanoid.io/ontology/CriticalVulnerability
:CriticalVulnerability rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( <http://d3fend.mitre.org/ontologies/d3fend.owl#Vulnerability>
                                                                  [ rdf:type owl:Class ;
                                                                    owl:unionOf ( [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                                           owl:onProperty :cvssBaseScore ;
                                                                                                           owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                                                                owl:onDatatype xsd:decimal ;
                                                                                                                                owl:withRestrictions ( [ xsd:minInclusive 9.0
                                                                                                                                                       ]
                                                                                                                                                     )
                                                                                                                              ]
                                                                                                         ]
                                                                                                         [ rdf:type owl:Restriction ;
                                                                                                           owl:onProperty :cvssBaseScore ;
                                                                                                           owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                                                                owl:onDatatype xsd:decimal ;
                                                                                                                                owl:withRestrictions ( [ xsd:maxInclusive 10.0
                                                                                                                                                       ]
                                                                                                                                                     )
                                                                                                                              ]
                                                                                                         ]
                                                                                                       ) ;
                                                                                    rdf:type owl:Class
                                                                                  ]
                                                                                  [ rdf:type owl:Restriction ;
                                                                                    owl:onProperty :severityRating ;
                                                                                    owl:hasValue :CVSS-SeverityType-Critical
                                                                                  ]
                                                                                )
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:subClassOf <http://d3fend.mitre.org/ontologies/d3fend.owl#Vulnerability> .


###  https://mekanoid.io/ontology/HighVulnerability
:HighVulnerability rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( <http://d3fend.mitre.org/ontologies/d3fend.owl#Vulnerability>
                                                              [ rdf:type owl:Class ;
                                                                owl:unionOf ( [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                                       owl:onProperty :cvssBaseScore ;
                                                                                                       owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                                                            owl:onDatatype xsd:decimal ;
                                                                                                                            owl:withRestrictions ( [ xsd:minInclusive 7.0
                                                                                                                                                   ]
                                                                                                                                                 )
                                                                                                                          ]
                                                                                                     ]
                                                                                                     [ rdf:type owl:Restriction ;
                                                                                                       owl:onProperty :cvssBaseScore ;
                                                                                                       owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                                                            owl:onDatatype xsd:decimal ;
                                                                                                                            owl:withRestrictions ( [ xsd:maxExclusive 9.0
                                                                                                                                                   ]
                                                                                                                                                 )
                                                                                                                          ]
                                                                                                     ]
                                                                                                   ) ;
                                                                                rdf:type owl:Class
                                                                              ]
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty :severityRating ;
                                                                                owl:hasValue :CVSS-AvailabilityImpact-High
                                                                              ]
                                                                            )
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                   rdfs:subClassOf <http://d3fend.mitre.org/ontologies/d3fend.owl#Vulnerability> .



###  https://mekanoid.io/ontology/cpe
:cpe rdf:type owl:DatatypeProperty ;
     rdfs:subPropertyOf <http://d3fend.mitre.org/ontologies/d3fend.owl#identifier> ;
     rdfs:domain <http://d3fend.mitre.org/ontologies/d3fend.owl#Software> ;
     rdfs:range xsd:string .


###  https://mekanoid.io/ontology/cvssBaseScore
:cvssBaseScore rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain <http://d3fend.mitre.org/ontologies/d3fend.owl#Vulnerability> ;
               rdfs:range xsd:decimal .


###  https://mekanoid.io/ontology/cvssExploitabilityScore
:cvssExploitabilityScore rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf owl:topDataProperty ;
                         rdf:type owl:FunctionalProperty ;
                         rdfs:domain <http://d3fend.mitre.org/ontologies/d3fend.owl#Vulnerability> ;
                         rdfs:range xsd:decimal .


###  https://mekanoid.io/ontology/cvssImpactScore
:cvssImpactScore rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdf:type owl:FunctionalProperty ;
                 rdfs:domain <http://d3fend.mitre.org/ontologies/d3fend.owl#Vulnerability> ;
                 rdfs:range xsd:decimal .

###  https://mekanoid.io/ontology/attackComplexity
:attackComplexity rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :hasCVSSMetric ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:range :CVSS-AttackComplexity .


###  https://mekanoid.io/ontology/attackVector
:attackVector rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :hasCVSSMetric ;
              rdf:type owl:FunctionalProperty ;
              rdfs:range :CVSS-AttackVector .


###  https://mekanoid.io/ontology/availabilityImpact
:availabilityImpact rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :hasCVSSMetric ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:range :CVSS-AvailabilityImpact .


###  https://mekanoid.io/ontology/confidentialityImpact
:confidentialityImpact rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :hasCVSSMetric ;
                       rdf:type owl:FunctionalProperty .


###  https://mekanoid.io/ontology/exploitCodeMaturity
:exploitCodeMaturity rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :hasCVSSMetric ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:range :CVSS-ExploitCodeMaturity .


###  https://mekanoid.io/ontology/hasCVSSMetric
:hasCVSSMetric rdf:type owl:ObjectProperty ;
               rdfs:domain <http://d3fend.mitre.org/ontologies/d3fend.owl#Vulnerability> .


mknd:CVSS-ExploitCodeMaturity-NotDefined
  rdf:type         owl:NamedIndividual , mknd:CVSS-ExploitCodeMaturity;
  rdfs:label       "Not Defined (X)";
  skos:altLabel    "E:X";
  skos:definition  "Assigning this value indicates there is insufficient information to choose one of the other values, and has no impact on the overall Temporal Score, i.e., it has the same effect on scoring as assigning High.";
  mknd:value       "X" .

mknd:CVSS-AttackComplexity
  rdf:type             owl:Class;
  rdfs:label           "Attack Complexity";
  rdfs:subClassOf      mknd:CVSS;
  owl:equivalentClass  [ rdf:type   owl:Class;
    owl:oneOf  ( mknd:CVSS-AttackComplexity-High mknd:CVSS-AttackComplexity-Low )
    ];
  skos:altLabel        "AC";
  skos:definition      "This metric describes the conditions beyond the attacker’s control that must exist in order to exploit the vulnerability. As described below, such conditions may require the collection of more information about the target, or computational exceptions. Importantly, the assessment of this metric excludes any requirements for user interaction in order to exploit the vulnerability (such conditions are captured in the User Interaction metric). If a specific configuration is required for an attack to succeed, the Base metrics should be scored assuming the vulnerable component is in that configuration. The Base Score is greatest for the least complex attacks." .

mknd:CVSS-SecurityRequirement-High
  rdf:type         owl:NamedIndividual , mknd:CVSS-SecurityRequirement;
  rdfs:label       "High";
  skos:definition  "Loss of [Confidentiality | Integrity | Availability] is likely to have a catastrophic adverse effect on the organization or individuals associated with the organization (e.g., employees, customers)." .

mknd:CVSS-SeverityType
  rdf:type             owl:Class;
  rdfs:comment         "Qualitative Severity Rating";
  rdfs:label           "Severity Type";
  rdfs:subClassOf      mknd:CVSS;
  owl:equivalentClass  [ rdf:type   owl:Class;
    owl:oneOf  ( mknd:CVSS-SeverityType-Critical mknd:CVSS-SeverityType-High mknd:CVSS-SeverityType-Low mknd:CVSS-SeverityType-Medium mknd:CVSS-SeverityType-None )
    ];
  skos:definition      "Specifies the CVSS base, temporal, threat, or environmental severity type." .

[ rdf:type             owl:AllDifferent;
    owl:distinctMembers  ( mknd:CVSS-RemediationLevel-NotDefined mknd:CVSS-RemediationLevel-OfficialFix mknd:CVSS-RemediationLevel-TemporaryFix mknd:CVSS-RemediationLevel-Unavailable mknd:CVSS-RemediationLevel-Workaround )
    ] .

mknd:CVSS-ReportConfidence
  rdf:type             owl:Class;
  rdfs:label           "Report Confidence";
  rdfs:subClassOf      mknd:CVSS;
  owl:equivalentClass  [ rdf:type   owl:Class;
    owl:oneOf  ( mknd:CVSS-ReportConfidence-Confirmed mknd:CVSS-ReportConfidence-NotDefined mknd:CVSS-ReportConfidence-Reasonable mknd:CVSS-ReportConfidence-Unknown )
    ];
  skos:altLabel        "RC";
  skos:definition      "This metric measures the degree of confidence in the existence of the vulnerability and the credibility of the known technical details. Sometimes only the existence of vulnerabilities is publicized, but without specific details. For example, an impact may be recognized as undesirable, but the root cause may not be known. The vulnerability may later be corroborated by research which suggests where the vulnerability may lie, though the research may not be certain. Finally, a vulnerability may be confirmed through acknowledgment by the author or vendor of the affected technology. The urgency of a vulnerability is higher when a vulnerability is known to exist with certainty. This metric also suggests the level of technical knowledge available to would-be attackers. The more a vulnerability is validated by the vendor or other reputable sources, the higher the score." .

mknd:CVSS-PrivilegesRequired
  rdf:type             owl:Class;
  rdfs:label           "Privileges Required";
  rdfs:subClassOf      mknd:CVSS;
  owl:equivalentClass  [ rdf:type   owl:Class;
    owl:oneOf  ( mknd:CVSS-PrivilegesRequired-High mknd:CVSS-PrivilegesRequired-Low mknd:CVSS-PrivilegesRequired-None )
    ];
  skos:altLabel        "PR";
  skos:definition      "This metric describes the level of privileges an attacker must possess before successfully exploiting the vulnerability. The Base Score is greatest if no privileges are required." .

mknd:LowSeverityVulnerability
  rdf:type             owl:Class;
  rdfs:label           "Low Severity Vulnerability";
  rdfs:subClassOf      d3f:Vulnerability;
  owl:equivalentClass  [ rdf:type            owl:Class;
    owl:intersectionOf  ( d3f:Vulnerability
      [ rdf:type     owl:Class;
        owl:unionOf  ( [ rdf:type            owl:Class;
            owl:intersectionOf  ( [ rdf:type            owl:Restriction;
                owl:onProperty      mknd:cvssBaseScore;
                owl:someValuesFrom  [ rdf:type              rdfs:Datatype;
                  owl:onDatatype        <http://www.w3.org/2001/XMLSchema#decimal>;
                  owl:withRestrictions  ( [ <http://www.w3.org/2001/XMLSchema#minExclusive>
                      0.0 ]
                    )
                  ]
                ]
              [ rdf:type            owl:Restriction;
                owl:onProperty      mknd:cvssBaseScore;
                owl:someValuesFrom  [ rdf:type              rdfs:Datatype;
                  owl:onDatatype        <http://www.w3.org/2001/XMLSchema#decimal>;
                  owl:withRestrictions  ( [ <http://www.w3.org/2001/XMLSchema#maxExclusive>
                      4.0 ]
                    )
                  ]
                ]
              )
            ]
          [ rdf:type        owl:Restriction;
            owl:hasValue    mknd:CVSS-AttackComplexity-Low;
            owl:onProperty  mknd:severityRating
            ]
          )
        ]
      )
    ] .

[ rdf:type             owl:AllDifferent;
    owl:distinctMembers  ( mknd:CVSS-AttackVector-Adjacent mknd:CVSS-AttackVector-Local mknd:CVSS-AttackVector-Network mknd:CVSS-AttackVector-Physical )
    ] .

