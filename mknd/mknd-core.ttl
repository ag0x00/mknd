###  https://mekanoid.io/ontology/Detection
:Detection rdf:type owl:Class ;
           rdfs:subClassOf <http://d3fend.mitre.org/ontologies/d3fend.owl#DigitalEvent> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://d3fend.mitre.org/ontologies/d3fend.owl#produces> ;
                             owl:someValuesFrom :ActionableItem
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://d3fend.mitre.org/ontologies/d3fend.owl#created-by> ;
                             owl:allValuesFrom :DetectionRule
                           ] .


###  https://mekanoid.io/ontology/DetectionRule
:DetectionRule rdf:type owl:Class ;
               rdfs:subClassOf <http://d3fend.mitre.org/ontologies/d3fend.owl#ApplicationRule> ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://d3fend.mitre.org/ontologies/d3fend.owl#creates> ;
                                 owl:someValuesFrom :Detection
                               ] .


