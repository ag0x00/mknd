###  https://mekanoid.io/ontology/CommunicationProtocol
:CommunicationProtocol rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Class ;
                                             owl:oneOf ( :ICMP
                                                         :TCP
                                                         :UDP
                                                       )
                                           ] ;
                       rdfs:subClassOf :MekaThing ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty <http://d3fend.mitre.org/ontologies/d3fend.owl#used-by> ;
                                         owl:someValuesFrom <http://d3fend.mitre.org/ontologies/d3fend.owl#NetworkPackets>
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty <http://d3fend.mitre.org/ontologies/d3fend.owl#used-by> ;
                                         owl:someValuesFrom <http://d3fend.mitre.org/ontologies/d3fend.owl#NetworkService>
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty <http://d3fend.mitre.org/ontologies/d3fend.owl#used-by> ;
                                         owl:someValuesFrom <http://d3fend.mitre.org/ontologies/d3fend.owl#NetworkSession>
                                       ] .

mknd:CommunicationProtocol
  rdf:type             owl:Class;
  rdfs:isDefinedBy     "https://en.wikipedia.org/wiki/Communication_protocol";
  rdfs:subClassOf      mknd:MekaThing ;
  rdfs:subClassOf      [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:used-by;
    owl:someValuesFrom  d3f:NetworkSession
    ];
  rdfs:subClassOf      [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:used-by;
    owl:someValuesFrom  d3f:NetworkService
    ];
  rdfs:subClassOf      [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:used-by;
    owl:someValuesFrom  d3f:NetworkPackets
    ];
  owl:equivalentClass  [ rdf:type   owl:Class;
    owl:oneOf  ( mknd:ICMP mknd:TCP mknd:UDP )
    ] .

