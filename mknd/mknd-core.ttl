              rdfs:domain <http://d3fend.mitre.org/ontologies/d3fend.owl#Vulnerability> ;
              rdfs:range xsd:string .


###  https://mekanoid.io/ontology/vendorProperty
:vendorProperty rdf:type owl:DatatypeProperty .


###  https://mekanoid.io/ontology/version
:version rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdf:type owl:FunctionalProperty ;
         rdfs:range xsd:decimal .

mknd:Task  rdf:type      owl:Class;
  skos:definition  "A task is a single unit of work that enables the automation of defensive techniques."@en;
  rdfs:subClassOf  mknd:SystemObject;
  rdfs:subClassOf  [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:implements;
    owl:someValuesFrom  d3f:DefensiveTechnique;
    rdfs:comment    "A task implements defensive techniques."@en
    ];
  rdfs:subClassOf  [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:may-produce;
    owl:someValuesFrom  d3f:DigitalArtifact;
    rdfs:comment    "A task may produce digital artifacts as a result of its execution."@en
    ];
  rdfs:subClassOf  [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:enables;
    owl:someValuesFrom  td:InteractionAffordance;
    rdfs:comment    "A task enables interaction affordances to individual digital artifacts."@en
    ];
  rdfs:subClassOf  [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:records;
    owl:someValuesFrom  mknd:TaskRun ;
    rdfs:comment    "A task records metadata about its execution in a task run."@en
    ];
  rdfs:subClassOf  [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:accesses;    
    owl:someValuesFrom  d3f:WebAPIResource ;
    rdfs:comment    "A task accesses web API resources."@en
    ];
  rdfs:subClassOf  [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:uses;    
    owl:someValuesFrom  mknd:TaskTemplate;
    rdfs:comment    "A task uses task templates to define the parameters of a task."@en
    ];
  rdfs:subClassOf  [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:uses;    
    owl:someValuesFrom  mknd:DataTemplate;
    rdfs:comment    "A task uses data templates to map the output of a task to digital artifacts."@en
    ];
  rdfs:subClassOf  [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:uses;    
    owl:someValuesFrom  mknd:Connection;
    rdfs:comment    "A task uses a connection to authenticate third party software services or APIs."@en
    ];
  rdfs:label       "Task"@en;
  skos:altLabel    "Task" .

mknd:TaskRun  rdf:type   owl:Class;
  rdfs:label       "Task Run"@en;
  skos:definition "A task run records information about the execution of a task."@en;
  rdfs:subClassOf  mknd:SystemObject , d3f:EventLog .

mknd:DataTemplate  rdf:type  owl:Class;
  skos:definition  "A data template maps the output of a task to digital artifacts."@en;
  rdfs:label       "Data Template";
  rdfs:subClassOf  mknd:SystemObject;
  rdfs:subClassOf  [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:maps;
    owl:someValuesFrom  d3f:WebAPIResource ;
    rdfs:comment    "A data template maps the output of a task to web API resources."@en
    ] .

mknd:TaskTemplate  rdf:type  owl:Class;
  skos:definition  "A task template is a blueprint for the parameters of a task."@en;
  rdfs:label       "Task Template";
  skos:related     td:InteractionAffordance ;
  rdfs:subClassOf  mknd:SystemObject .

mknd:TaskPattern  rdf:type  owl:Class;
  skos:definition  "A task pattern is a rule that triggers the execution of automated task runs. Initially task patterns are implemented as conjunctive clauses using triple-based patterns of [subject, predicate, object] or [entity attribute value] querying the digital artifacts in your knowledge graph using the D3FEND ontology. The vision is that task patterns could be implemented using machine learning, and statistical methods, in addition to symbolic logic in order to capture complex requirements which may be difficult to encode using triple-based patterns." ;  
  rdfs:label       "Task Pattern";
  rdfs:subClassOf  mknd:SystemObject;
  rdfs:subClassOf  [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:filters ;
    owl:someValuesFrom  d3f:Database ;
    rdfs:comment    "A task pattern filters knowledge graphs to identify digital artifacts which may conditionally trigger the execution of automated task runs."@en
    ];
  rdfs:subClassOf  [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:may-invoke ;
    owl:someValuesFrom  mknd:AutomatedTaskRun ;
    rdfs:comment    "A task pattern may invoke automated task runs when its filters are satisfied."@en
    ] .

mknd:ManualTaskRun  rdf:type  owl:Class;
  rdfs:label       "Manual Task Run";
  rdfs:subClassOf  mknd:TaskRun , d3f:UserAction .

mknd:AutomatedTaskRun
  rdf:type         owl:Class;
  rdfs:label       "Automated Task Run";
  rdfs:subClassOf  mknd:TaskRun , mknd:MachineAction;
  rdfs:subClassOf  [ rdf:type           owl:Restriction;
    owl:allValuesFrom  mknd:TaskPattern;
    owl:onProperty     d3f:invoked-by
    ] .

mknd:Connection  rdf:type  owl:Class;
  skos:definition  "A third party software service or API used to enable the automation of defensive techniques." ;
  rdfs:label       "Connection"@en;
  rdfs:subClassOf  mknd:SystemObject;
  rdfs:subClassOf  [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:enables;
    owl:someValuesFrom  mknd:Task ;
    rdfs:comment    "A connection enables tasks to be executed."@en
    ] ;
  rdfs:subClassOf  [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:contains;
    owl:someValuesFrom  d3f:AccessToken ;
    rdfs:comment    "A connection is configured with access tokens that must be protected."@en
    ] .

mknd:supports  rdf:type  owl:ObjectProperty;
  rdfs:label          "supports"@en;
  rdfs:domain         mknd:Connection;
  rdfs:range          [ rdf:type     owl:Class;
    owl:unionOf  ( d3f:SoftwareProduct d3f:SoftwareService )
    ];
  skos:example  "Connection x supports product or service y." .

[ rdf:type     owl:AllDisjointClasses;
    owl:members  ( mknd:Task mknd:TaskPattern mknd:TaskRun mknd:TaskTemplate )
    ] .

mknd:Payload  rdf:type   owl:Class;
  rdfs:label       "Payload"@en;
  rdfs:subClassOf  mknd:MekaThing .

mknd:IOCRule  rdf:type   owl:Class;
  rdfs:label       "IOC";
  rdfs:subClassOf  mknd:DetectionRule .

mknd:value  rdf:type        owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:domain         mknd:MekaThing;
  rdfs:range          xsd:string;
  rdfs:comment        "A value parameter to be passed to Tasks"@en;
  rdfs:label          "has value"@en .

mknd:indicatedBy  rdf:type  owl:ObjectProperty;
  rdfs:label          "indicated by"@en;
  rdfs:subPropertyOf  mknd:mayBeIndicatedBy;
  owl:inverseOf       mknd:indicates .

mknd:mayBeIndicatedBy rdf:type owl:ObjectProperty ;
                  owl:inverseOf mknd:mayIndicate ;
                  rdfs:label "may be indicated by"@en .

mknd:UDP  rdf:type        mknd:CommunicationProtocol , owl:NamedIndividual;
  rdfs:isDefinedBy  "https://en.wikipedia.org/wiki/User_Datagram_Protocol";
  rdfs:label        "UDP";
  skos:altLabel     "User Datagram Protocol"@en .

mknd:lastStatusChangeEpoch
  rdf:type            owl:DatatypeProperty;
  rdfs:subPropertyOf  mknd:dateTimeEpoch ;
  rdfs:label          "last status change"@en .
  

mknd:SHA256  rdf:type    owl:Class;
  rdfs:comment     "SHA-256 cryptographic hash value"@en;
  rdfs:label       "SHA-256 Hash"@en;
  rdfs:subClassOf  mknd:Hash .

mknd:PublicIPAddress  rdf:type  owl:Class;
  rdfs:comment      "A public IP address is a globally routable unicast IP address, meaning that the address is not an address reserved for use in private networks, such as those reserved by RFC 1918, or the various IPv6 address formats of local scope or site-local scope, for example for link-local addressing. Public IP addresses may be used for communication between hosts on the global Internet. In a home situation, a public IP address is the IP address assigned to the home's network by the ISP. In this case, it is also locally visible by logging into the router configuration."@en;
  rdfs:isDefinedBy  "https://en.wikipedia.org/wiki/IP_address#Public_address"^^xsd:anyURI;
  rdfs:label        "Public IP Address"@en;
  rdfs:subClassOf   d3f:IPAddress;
  skos:example      "8.8.8.8" , "4.2.2.1" .

mknd:firstSeen  rdf:type    owl:DatatypeProperty;
  rdfs:comment        "The first time the enity had been observed by the application."@en;
  rdfs:label          "first seen"@en;
  rdfs:subPropertyOf  mknd:timeSeen .

mknd:SSL-Service  rdf:type  mknd:NetworkServiceProtocol , owl:NamedIndividual;
  rdfs:label     "SSL Service";
  skos:altLabel  "HTTPS Service" .

mknd:extRisk  rdf:type      owl:DatatypeProperty;
  rdfs:comment        "Risk as defined by the 3rd party vendor responsible for creating the Artifact."@en;
  rdfs:label          "Risk"@en;
  rdfs:subPropertyOf  mknd:extClassification .

mknd:Hash  rdf:type      owl:Class;
  rdfs:comment     "A hash is a grouping of characteristics unique to the result of applying a mathematical algorithm that maps data of arbitrary size to a bit string (the 'hash') and is a one-way function, that is, a function which is practically infeasible to invert. This is commonly used for integrity checking of data. [based on https://en.wikipedia.org/wiki/Cryptographic_hash_function]"@en;
  rdfs:label       "Hash"@en;
  rdfs:subClassOf  mknd:MekaThing , d3f:Identifier;
  rdfs:subClassOf  [ rdf:type                  owl:Restriction;
    owl:onDataRange           xsd:hexBinary;
    owl:onProperty            mknd:value;
    owl:qualifiedCardinality  "1"^^xsd:nonNegativeInteger
    ];
  rdfs:subClassOf  [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:associated-with;
    owl:someValuesFrom  mknd:Verdict
    ] .
    
mknd:lastSeenEpoch  rdf:type  owl:DatatypeProperty;
  rdfs:label          "last seen"@en;
  rdfs:subPropertyOf  mknd:dateTimeEpoch .

mknd:Report  rdf:type    owl:Class;
  rdfs:label       "Analysis Report"@en;
  rdfs:subClassOf  mknd:MekaThing , d3f:Document;
  rdfs:subClassOf  [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:analyzes;
    owl:someValuesFrom  d3f:DomainName
    ];
  rdfs:subClassOf  [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:analyzes;
    owl:someValuesFrom  mknd:EmailAddress
    ];
  rdfs:subClassOf  [ rdf:type           owl:Restriction;
    owl:allValuesFrom  mknd:Verdict;
    owl:onProperty     d3f:produces
    ];
  rdfs:subClassOf  [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:analyzes;
    owl:someValuesFrom  d3f:IPAddress
    ];
  rdfs:subClassOf  [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:analyzes;
    owl:someValuesFrom  d3f:FileHash
    ];
  rdfs:subClassOf  [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:analyzes;
    owl:someValuesFrom  mknd:Socket
    ];
  rdfs:subClassOf  [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:analyzes;
    owl:someValuesFrom  d3f:URL
    ];
  rdfs:subClassOf  [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:analyzes;
    owl:someValuesFrom  mknd:Hash
    ];
  skos:definition  "A document containing the results of an assessment or analysis of an artifact." .

mknd:extThreat  rdf:type    owl:DatatypeProperty;
  rdfs:comment        "Threat Level as defined by the 3rd party vendor responcible for creating the Artifact."@en;
  rdfs:label          "Threat Level"@en;
  rdfs:subPropertyOf  mknd:extClassification .

mknd:IncidentStatus-Unknown
  rdf:type    mknd:IncidentStatus , owl:NamedIndividual;
  rdfs:label  "Unknown" .

mknd:lastAccessedEpoch
  rdf:type            owl:DatatypeProperty;
  rdfs:label          "last accessed"@en;
  rdfs:subPropertyOf  mknd:dateTimeEpoch .

mknd:ICMP  rdf:type  mknd:CommunicationProtocol , owl:NamedIndividual;
  rdfs:label  "ICMP" .

mknd:IncidentStatus-Open
  rdf:type    mknd:IncidentStatus , owl:NamedIndividual;
  rdfs:label  "Open" .

mknd:id  rdf:type     owl:InverseFunctionalProperty , owl:DatatypeProperty;
  rdfs:domain         mknd:MekaThing;
  rdfs:range          xsd:string;
  rdfs:label          "ID"@en;
  rdfs:subPropertyOf  d3f:identifier;
  skos:definition     "Mekanoid ID" .

mknd:Detection  rdf:type  owl:Class;
  rdfs:label       "Finding";
  rdfs:subClassOf  d3f:DigitalEvent;
  rdfs:subClassOf  [ rdf:type           owl:Restriction;
    owl:allValuesFrom  mknd:DetectionRule;
    owl:onProperty     d3f:created-by
    ];
  rdfs:subClassOf  [ rdf:type            owl:Restriction;
    owl:onProperty      d3f:produces;
    owl:someValuesFrom  mknd:ActionableItem
    ];
